<!DOCTYPE suppressions PUBLIC
  "-//Checkstyle//DTD SuppressionFilter Configuration 1.2//EN"
  "https://checkstyle.org/dtds/suppressions_1_2.dtd">

<suppressions>

  <!-- TODO: Rename package in 3.0.0 release -->
  <suppress
    checks="PackageName"
    message="'com.aoapps.payments.authorizeNet'"
  />

  <!-- TODO: Implement tests -->
  <suppress
    checks="AbbreviationAsWordInName"
    files="[/\\]com[/\\]aoapps[/\\]payments[/\\]authorizeNet[/\\]AuthorizeNetTestTODO\.java$"
    message="'AuthorizeNetTestTODO'"
  />

  <!-- Will keep the names matching the Authorize.Net documentation for clarity -->
  <suppress
    checks="(MemberName|ParameterName)"
    files="[/\\]com[/\\]aoapps[/\\]payments[/\\]authorizeNet[/\\]AuthorizeNet\.java$"
    message="'(x_login|x_tran_key|x_type)'"
  />
  <suppress
    checks="MethodName"
    files="[/\\]com[/\\]aoapps[/\\]payments[/\\]authorizeNet[/\\]AuthorizeNet\.java$"
    message="'(getX_login|getX_tran_key)'"
  />

</suppressions>
